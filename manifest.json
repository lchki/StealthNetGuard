{
  "manifest_version": 3,
  "name": "My Proxy Extension",
  "version": "1.0",
  "description": "My Chrome Proxy Extension",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "permissions": ["proxy"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
